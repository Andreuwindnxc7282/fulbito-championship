{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "app/[[...slug]]/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "https://fulbito-backend.onrender.com/api",
    "NEXT_PUBLIC_BASE_URL": "https://fulbito-backend.onrender.com"
  }
}
